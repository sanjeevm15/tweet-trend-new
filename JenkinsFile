pipeline {
    agent {
        node {
            label 'maven'
        }
    }

    stages {
        stage("code-clone") {
            steps {
                git branch: 'main', url: 'https://github.com/sanjeevm15/tweet-trend-new.git'
            }
        }

        stage("Build") {
            steps {
                sh 'mvn clean deploy'
            }
        }
    }
}

